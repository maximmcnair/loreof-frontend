.topicSelect
  background #53606c url('/images/topic-select.png')
  background-attachment fixed
  .content
    padding 50px 0px

.topicSelect-topics
  padding 20px 0px
  text-align center
  .btn
    margin 10px 10px

.topicSelect-subtitle
  text-align center
  color #2D3237
  font-size 26px
